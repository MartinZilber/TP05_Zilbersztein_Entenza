@{
ViewData["Title"] = "Habitación 9";
}
<h1>Corrientes Subterráneas</h1>
<h3>LA SALA DE ENSAYO</h3>
<p>@ViewBag.Dato</p>
<p>Vidas: @ViewBag.Vidas</p>
<p>Habiendo salido de la sala de ensayo, </p>
<div class="columnas">
    <img src="/images/habitacion9.jpg">
    <audio controls src="/audios/AudioHabitacion8.mp4">Audio</audio>
    <form action='@Url.Action("Habitacion", "Home")'>
        <input type="hidden" name="sala" value=9>
        <input autocomplete="off" type="text" name="clave" placeholder="" required><br>
        <input class="btn btn-primary" type="submit" name="Validar">
        <input class="btn btn-secondary" type ="reset" name ="Resetear">
    </form>
    <div>
        <button onclick="alert('¿Cuáles de los objetos pueden ser relacionados con juegos?')">Pista 1</button>
        <button onclick="alert('Presta atención a los objetos de madera')">Pista 2</button>
    </div>
</div>